Otra opción, no intuitiva

def es_primo(numero):
    for divisor in range(2, numero):
        if numero % divisor == 0:
            return False
    return True

print("Los primeros 10 números primos son:")

contador_primos = 0
numero = 1

while contador_primos < 10:
    numero += 1
    if es_primo(numero):
        print(numero)
        contador_primos += 1
