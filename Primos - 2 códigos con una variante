def es_primo(numero):
  if numero <= 1:
      return False
  if numero <= 3:
      return True
  if numero % 2 == 0 or numero % 3 == 0:
      return False
  if numero % 2 >= 0 or numero % 3 >= 0:
    return True
  
print("Los primeros 10 números primos son:")

contador_primos = 0
numero = 1

while contador_primos < 10:
  numero += 1
  if es_primo(numero):
      print(numero)
      contador_primos += 1

